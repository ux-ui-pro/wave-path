{
  "name": "wave-path",
  "version": "0.0.1",
  "description": "Multi-layered SVG shape overlays that are dynamically generated with adjustable properties are used in creating transitions in SPA or for designing drawer menus",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/wave-path.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/wave-path/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/wave-path",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "lint:ts": "eslint src/**/*.ts",
    "lint:fix:ts": "eslint src/**/*.ts --fix",
    "format:ts": "prettier --write src/**/*.ts",
    "lint:fix": "yarn lint:fix:ts && yarn format:ts"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@rollup/plugin-terser": "0.4.4",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.2",
    "vite": "7.1.3",
    "vite-plugin-dts": "4.5.4",
    "gsap": "3.13.0"
  },
  "peerDependencies": {
    "gsap": ">=3.12.0"
  },
  "keywords": [
    "shape",
    "overlays",
    "GSAP",
    "transition",
    "SVG"
  ]
}
